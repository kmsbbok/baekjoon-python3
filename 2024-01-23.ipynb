    "N, M = map(int, input().split())\n",
    "basket = []\n",
    "\n",
    "for a in range(N):\n",
    "    basket += '0'\n",
    "\n",
    "for b in range(M):\n",
    "    i, j, k = map(int, input().split())\n",
    "    for c in range(i, j + 1):\n",
    "        basket[c - 1] = k\n",
    "\n",
    "for d in range(N):\n",
    "    print(basket[d], end = ' ')"
   ]
