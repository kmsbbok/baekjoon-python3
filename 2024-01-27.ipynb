    "N = int(input())\n",
    "number = int(input())\n",
    "result = 0\n",
    "\n",
    "for i in range(N):\n",
    "    globals()[i] = (number // 10 ** (N - 1 - i)) % 10\n",
    "    result += globals()[i]\n",
    "\n",
    "print(result)"
